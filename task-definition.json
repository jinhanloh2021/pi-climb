      "image": "842832773369.dkr.ecr.ap-southeast-1.amazonaws.com/pi-climb-dev-server:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pi-climb_task",
          "awslogs-create-group": "true",
