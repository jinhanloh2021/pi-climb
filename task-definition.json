          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "name": "go_server",
      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 8080,
          "hostPort": 8080,
          "name": "go_server-8080-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:842832773369:secret:pi-climb/dev-wXQQeQ:JWT_SECRET::"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:842832773369:secret:pi-climb/dev-wXQQeQ:DATABASE_URL::"
        },
        {
          "name": "SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:842832773369:secret:pi-climb/dev-wXQQeQ:SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET::"
        },
        {
          "name": "SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-1:842832773369:secret:pi-climb/dev-wXQQeQ:SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID::"
        }
      ],
      "systemControls": [],
      "volumesFrom": []
    },
    {
      "cpu": 0,
      "environment": [],
      "essential": true,
      "image": "842832773369.dkr.ecr.ap-southeast-1.amazonaws.com/pi-climb-dev-web:latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/pi-climb_task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
