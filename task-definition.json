      "portMappings": [
        {
          "appProtocol": "http",
          "containerPort": 3000,
          "hostPort": 3000,
          "name": "nextjs_server-3000-tcp",
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "NEXT_PUBLIC_SUPABASE_URL",
